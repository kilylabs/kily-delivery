{
    "name": "kilylabs/kily-delivery",
    "type": "library",
    "description": "Manage deliveries using API. Calculate delivery cost for popular russian delivery compaines",
    "keywords": ["delivery", "calculation", "calculator", "ems", "post", "russia"],
    "homepage": "http://kily.ru/soft/kily-delivery",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Bogdanov",
            "email": "ab@dev.kily.ru"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "ext-libxml": "*",
        "guzzlehttp/guzzle": "6.*",
        "mascame/arrayer": "3.*",
        "geocoder-php/google-maps-provider": "^4.4",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/message": "^1.7",
        "geocoder-php/yandex-provider": "^4.2",
        "gietos/dostavista": "^0.11.0",
        "dvdoug/boxpacker": "^3.4"
    },
    "autoload": {
        "psr-4": {"Kily\\Delivery\\": "src/Delivery"}
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "fzaninotto/faker": "1.*"
    }
}
